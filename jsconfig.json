{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "root/*": ["./*"],
      "components/*": ["src/components/*"],
      "containers/*": ["src/containers/*"],
      "images/*": ["src/images/*"],
      "actions/*": ["src/redux/actions/*"],
      "constants/*": ["src/redux/constants/*"],
      "reducers/*": ["src/redux/reducers/*"],
      "selectors/*": ["src/redux/selectors/*"]
    },
    "jsx": "react"
  },
  "exclude": ["node_modules", "build"]
}
